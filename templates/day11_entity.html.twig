{#
/**
 * @file day11_entity.html.twig
 * Default theme implementation to present Day11entity data.
 *
 * This template is used when viewing Day11entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_day11_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('day11_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
